<core:FragmentDefinition
    xmlns:upload="sap.m.upload"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
   
>
    <Dialog
        title="List of PO From ERP "
        contentWidth="400px"
    >
        <content>
         <List
                items="{PO>/}"
                headerText=""
                growing="true"
                growingThreshold="4"
                growingScrollToLoad="false"
            >
                <StandardListItem title="{PO>poNo}" />
            </List>
      </content>
       <footer>
            <Toolbar>
                <ToolbarSpacer />
                
                <Button
                    text="Cancel"
                    press="onCancelPO"
                />
            </Toolbar>
            </footer>
           
    </Dialog>
</core:FragmentDefinition>